{
  "name": "falling-fruit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/tabs": "^0.13.0",
    "@reach/window-size": "^0.13.2",
    "@styled-icons/boxicons-regular": "^10.23.0",
    "@styled-icons/boxicons-solid": "^10.23.0",
    "axios": "^0.21.1",
    "google-map-react": "^2.1.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-split-pane": "^0.1.92",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.5",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@hack4impact-uiuc/eslint-plugin": "^2.0.10",
    "eslint": "^7.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "lint": "yarn lint:check --fix",
    "lint:check": "eslint src --ext js,jsx,ts,tsx",
    "format": "yarn format:output --write",
    "format:check": "yarn format:output --check",
    "format:output": "prettier \"./**/*.{js,jsx,ts,tsx,css,scss,json,md}\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,css,scss,json,md}": [
      "prettier --write"
    ],
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
